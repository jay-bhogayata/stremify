{
  "name": "stremify",
  "version": "1.0.0",
  "description": "A video streaming app",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "build": "npx tsc",
    "serve": "node --require ./dist/utils/tracing.js ./dist/index.js",
    "run:tracing": "pnpx ts-node --require ./src/utils/tracing.ts ./src/index.ts"
  },
  "keywords": [],
  "author": "Jay Bhogayata",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.44.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.50.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.50.0",
    "@opentelemetry/sdk-metrics": "^1.23.0",
    "@opentelemetry/sdk-node": "^0.50.0",
    "@opentelemetry/sdk-trace-node": "^1.23.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.2",
    "winston": "^3.13.0",
    "winston-loki": "^6.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.12.7",
    "drizzle-kit": "^0.20.14",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
