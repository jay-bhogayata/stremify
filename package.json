{
  "name": "stremify",
  "version": "1.0.0",
  "description": "A video streaming app",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "build": "tsc",
    "serve": "node ./dist/index.js",
    "test": "jest --verbose",
    "test:cov": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx ./src/database/migrate.ts",
    "db:studio": "npx drizzle-kit studio",
    "db:start": "docker run --name db_stremify -e POSTGRES_PASSWORD=pg123 -e POSTGRES_DB=stremify -dp 5432:5432 postgres:latest",
    "db:stop": "docker stop db_stremify",
    "db:remove": "docker rm db_stremify"
  },
  "keywords": [],
  "author": "Jay Bhogayata",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.565.0",
    "@aws-sdk/client-ses": "^3.556.0",
    "@aws-sdk/lib-storage": "^3.609.0",
    "@aws-sdk/s3-request-presigner": "^3.565.0",
    "@scalar/express-api-reference": "^0.4.80",
    "argon2": "^0.40.1",
    "body-parser": "^1.20.2",
    "connect-redis": "^7.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.1",
    "drizzle-zod": "^0.5.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-session": "^1.18.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "postgres": "^3.4.4",
    "redis": "^4.6.13",
    "swagger-jsdoc": "^6.2.8",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/uuid": "^9.0.8",
    "drizzle-kit": "^0.22.4",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
